{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "outputDirectory": ".",
  "buildCommand": "echo 'Build completed - static site ready'",
  "devCommand": "serve . -l 3000",
  "installCommand": "npm install",
  "framework": null,
  "public": true,
  "trailingSlash": false,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/pokedex",
      "destination": "/",
      "permanent": false
    }
  ]
}